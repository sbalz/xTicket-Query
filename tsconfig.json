{
    "compilerOptions": {
        "target": "es6",
        "lib": ["es2019", "dom"],
        "jsx": "react-jsx",
        "module": "es6",
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "allowJs": true,
        "isolatedModules": true,
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "types": []
    },
    "include": ["src/**/*", "src/Utils/.ts"]
}
